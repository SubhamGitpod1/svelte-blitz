{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"rootDir": ".",
		"baseUrl": ".",
		"target": "ES6",
		"moduleResolution": "classic"
	},
	"include": [
		"./**/*.ts",
		"./**/*.svelte"
	]
}
